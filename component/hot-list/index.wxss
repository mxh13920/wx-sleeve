@import '../../wxml/sleeve.wxss';

.container {
    width: 100%;
    background-color: #fff;
    display: flex;
    align-items: center;
    flex-direction: column;
    padding-top: 40rpx;
}

.container>image {
    width: 690rpx;
    height: 90rpx;
}

.inner-container {
    margin-top: 20rpx;
    display: flex;
    flex-direction: row;
    width: 100%;
    justify-content: space-between;
}

.right-container {
    display: flex;
    flex-direction: column;
    justify-content: space-between;
}

.left {
    display: flex;
    width: 346rpx;
    height: 522rpx;
}

.right-size {
    display: flex;
    width: 400rpx;
    height: 260rpx;
}